    "@microsoft/microsoft-graph-client": "^3.0.4",
    "@notionhq/client": "^2.2.1",
    "axios": "^1.2.1",
    "body-parser": "^1.20.2",
    "chalk": "^5.2.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonfile": "^6.1.0",
