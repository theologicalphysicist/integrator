{
  "name": "integrator-backend",
  "version": "0.1.0",
  "description": "express backend for integrator app",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "local": "ts-node-esm -r dotenv/config ./index.js dotenv_config_path=.env.local dotenv_config_debug=true",
    "debug": "DEBUG=express:* nodemon -x 'npm run local'",
    "test-compile": "tsc -p test/tsconfig.test.json --watch --pretty true",
    "dev": "nodemon -x 'npm run local'",
    "start": "node -r dotenv/config ./dist/index.js dotenv_config_path=./.env dotenv_config_debug=true",
    "jasmine-test": "",
    "node-test": "nodemon -x 'ts-node-esm ./test/*.test.ts'",
    "build": "",
    "spec": "npx ts-node-esm -r dotenv/config $SCRIPT dotenv_config_path=.env dotenv_config_debug=true"
  },
  "repository": {
    "type": "git",
